<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="android.com.vpn.MainActivity"
    android:background="#E1E0DE">

    <android.com.vpn.EditTextBtn
        android:id="@+id/btn"
        android:layout_width="370dp"
        android:layout_height="wrap_content"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:paddingTop="20dp"
        />
    <android.com.vpn.CustomWidget.SendButton
        android:id="@+id/sendbutton"
        android:layout_width="370dp"
        android:layout_height="wrap_content"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <android.com.vpn.ButtonLan
    android:id="@+id/buttonlan"
    android:layout_width="370dp"
    android:layout_height="wrap_content"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"/>
    <android.com.vpn.DiyView
        android:id="@+id/diyview"
        android:layout_width="370dp"
        android:layout_height="wrap_content"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        />

    <!-- 显示自定义控件返回的值 -->
</LinearLayout>
